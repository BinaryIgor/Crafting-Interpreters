var idx = 0;
var iterations = 5;

while (idx < iterations) {
  if (idx < 2) {
    print "Outer! " + idx;
  } else {
    var idxInner = 0;
    var iterationsInner = 3;
    while (idxInner < iterationsInner) {
      print "Inner! " + idxInner;
      if (idxInner > 1) {
        break;
      }
      idxInner = idxInner + 1;
    }
    break;
  }
  idx = idx + 1;
}